<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Dream X - No Search Results">
    <meta name="theme-color" content="#6366f1">
    <title>No Results Found | Dream X</title>
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/img/favicon.svg">
    <link rel="icon" type="image/png" sizes="32x32" href="/img/favicon-32x32.png">
    
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
            overflow: hidden;
            background: linear-gradient(135deg, #74ebd5 0%, #acb6e5 50%, #9796f0 100%);
            background-size: 400% 400%;
            animation: gradientShift 15s ease infinite;
            position: relative;
        }

        @keyframes gradientShift {
            0% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
            100% { background-position: 0% 50%; }
        }

        .container {
            position: relative;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            min-height: 100vh;
            padding: 2rem;
            z-index: 10;
        }

        /* Floating particles */
        .particle {
            position: absolute;
            border-radius: 50%;
            pointer-events: none;
            animation: float 20s infinite ease-in-out;
            opacity: 0.6;
        }

        @keyframes float {
            0%, 100% { transform: translate(0, 0) rotate(0deg); }
            33% { transform: translate(30px, -30px) rotate(120deg); }
            66% { transform: translate(-20px, 20px) rotate(240deg); }
        }

        /* Glowing orbs that follow mouse */
        .cursor-glow {
            position: fixed;
            width: 300px;
            height: 300px;
            border-radius: 50%;
            background: radial-gradient(circle, rgba(116, 235, 213, 0.3) 0%, transparent 70%);
            pointer-events: none;
            transform: translate(-50%, -50%);
            transition: all 0.15s ease-out;
            z-index: 1;
        }

        .cursor-glow-2 {
            width: 200px;
            height: 200px;
            background: radial-gradient(circle, rgba(151, 150, 240, 0.3) 0%, transparent 70%);
            transition: all 0.25s ease-out;
        }

        /* Main icon */
        .search-icon {
            font-size: 8rem;
            animation: swing 3s infinite ease-in-out;
            margin-bottom: 1rem;
        }

        @keyframes swing {
            0%, 100% { transform: rotate(-10deg); }
            50% { transform: rotate(10deg); }
        }

        .error-title {
            font-size: clamp(1.5rem, 4vw, 2.5rem);
            font-weight: 700;
            color: #fff;
            margin: 1rem 0 0.5rem;
            text-align: center;
            text-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
            animation: fadeInUp 0.8s ease-out;
        }

        .error-message {
            font-size: clamp(1rem, 2vw, 1.2rem);
            color: rgba(255, 255, 255, 0.9);
            text-align: center;
            margin-bottom: 2rem;
            max-width: 600px;
            text-shadow: 0 2px 10px rgba(0, 0, 0, 0.2);
            animation: fadeInUp 1s ease-out;
        }

        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(20px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        /* Floating emoji decorations */
        .floating-emoji {
            position: absolute;
            font-size: 3rem;
            animation: floatEmoji 4s infinite ease-in-out;
            user-select: none;
            transition: transform 0.3s ease;
        }

        .floating-emoji:hover {
            transform: scale(1.3) rotate(20deg);
            cursor: pointer;
        }

        @keyframes floatEmoji {
            0%, 100% { transform: translateY(0px) rotate(0deg); }
            50% { transform: translateY(-20px) rotate(10deg); }
        }

        .emoji-1 { top: 15%; left: 10%; animation-delay: 0s; }
        .emoji-2 { top: 25%; right: 15%; animation-delay: 0.5s; }
        .emoji-3 { bottom: 20%; left: 15%; animation-delay: 1s; }
        .emoji-4 { bottom: 30%; right: 10%; animation-delay: 1.5s; }
        .emoji-5 { top: 45%; left: 5%; animation-delay: 2s; }
        .emoji-6 { top: 55%; right: 8%; animation-delay: 2.5s; }

        /* Buttons */
        .button-group {
            display: flex;
            gap: 1.5rem;
            flex-wrap: wrap;
            justify-content: center;
            animation: fadeInUp 1.2s ease-out;
        }

        .btn {
            padding: 1rem 2.5rem;
            font-size: 1.1rem;
            font-weight: 700;
            text-decoration: none;
            border-radius: 50px;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            border: none;
            cursor: pointer;
            position: relative;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
            transform: translateY(0);
        }

        .btn::before {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            width: 0;
            height: 0;
            border-radius: 50%;
            background: rgba(255, 255, 255, 0.3);
            transform: translate(-50%, -50%);
            transition: width 0.6s, height 0.6s;
        }

        .btn:hover::before {
            width: 300px;
            height: 300px;
        }

        .btn:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 40px rgba(0, 0, 0, 0.4);
        }

        .btn:active {
            transform: translateY(-2px);
        }

        .btn-primary {
            background: linear-gradient(135deg, #74ebd5 0%, #acb6e5 100%);
            color: #333;
        }

        .btn-secondary {
            background: linear-gradient(135deg, #9796f0 0%, #fbc7d4 100%);
            color: #fff;
        }

        .btn span {
            position: relative;
            z-index: 1;
        }

        /* Interactive grid background */
        .grid-background {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-image: 
                linear-gradient(rgba(255, 255, 255, 0.05) 1px, transparent 1px),
                linear-gradient(90deg, rgba(255, 255, 255, 0.05) 1px, transparent 1px);
            background-size: 50px 50px;
            animation: gridMove 20s linear infinite;
            z-index: 0;
        }

        @keyframes gridMove {
            0% { transform: translate(0, 0); }
            100% { transform: translate(50px, 50px); }
        }

        /* Responsive */
        @media (max-width: 768px) {
            .emoji-1, .emoji-2, .emoji-3, .emoji-4 { display: none; }
            
            .button-group {
                flex-direction: column;
                width: 100%;
                max-width: 300px;
            }

            .btn {
                width: 100%;
            }
        }

        /* Sparkles */
        .sparkle {
            position: absolute;
            width: 4px;
            height: 4px;
            background: white;
            border-radius: 50%;
            animation: sparkle 2s infinite ease-in-out;
            pointer-events: none;
        }

        @keyframes sparkle {
            0%, 100% { opacity: 0; transform: scale(0); }
            50% { opacity: 1; transform: scale(1); }
        }

        /* Suggestions */
        .suggestions {
            background: rgba(255, 255, 255, 0.2);
            backdrop-filter: blur(10px);
            padding: 1.5rem 2rem;
            border-radius: 20px;
            margin-bottom: 2rem;
            animation: fadeInUp 1.4s ease-out;
            box-shadow: 0 5px 20px rgba(0, 0, 0, 0.2);
        }

        .suggestions h3 {
            color: #fff;
            margin-bottom: 0.5rem;
            font-size: 1.2rem;
        }

        .suggestions ul {
            list-style: none;
            color: rgba(255, 255, 255, 0.9);
            line-height: 1.8;
        }

        .suggestions li::before {
            content: 'üí° ';
            margin-right: 0.5rem;
        }
    </style>
</head>
<body>
    <div class="grid-background"></div>
    <div class="cursor-glow" id="cursorGlow1"></div>
    <div class="cursor-glow cursor-glow-2" id="cursorGlow2"></div>

    <div class="container">
        <!-- Floating Emojis -->
        <div class="floating-emoji emoji-1">üîé</div>
        <div class="floating-emoji emoji-2">ü§î</div>
        <div class="floating-emoji emoji-3">üìù</div>
        <div class="floating-emoji emoji-4">üéØ</div>
        <div class="floating-emoji emoji-5">üí≠</div>
        <div class="floating-emoji emoji-6">‚ú®</div>

        <!-- Main Content -->
        <div class="search-icon">üîç</div>
        <h2 class="error-title">No Results Found</h2>
        <p class="error-message">
            We couldn't find anything matching your search. 
            Try adjusting your search terms or explore different keywords.
        </p>

        <!-- Suggestions -->
        <div class="suggestions">
            <h3>Try these tips:</h3>
            <ul>
                <li>Check your spelling</li>
                <li>Use more general keywords</li>
                <li>Try different search terms</li>
                <li>Browse our community feed</li>
            </ul>
        </div>

        <!-- Action Buttons -->
        <div class="button-group">
            <button onclick="goBack()" class="btn btn-primary">
                <span>‚Üê Try Again</span>
            </button>
            <a href="/feed" class="btn btn-secondary">
                <span>Explore Feed üè†</span>
            </a>
        </div>
    </div>

    <script>
        // Cursor glow effect
        const glow1 = document.getElementById('cursorGlow1');
        const glow2 = document.getElementById('cursorGlow2');

        document.addEventListener('mousemove', (e) => {
            glow1.style.left = e.clientX + 'px';
            glow1.style.top = e.clientY + 'px';
            
            // Second glow with delay
            setTimeout(() => {
                glow2.style.left = e.clientX + 'px';
                glow2.style.top = e.clientY + 'px';
            }, 100);
        });

        // Create floating particles
        function createParticles() {
            const colors = ['#74ebd5', '#acb6e5', '#9796f0', '#fbc7d4', '#a1c4fd'];
            for (let i = 0; i < 20; i++) {
                const particle = document.createElement('div');
                particle.className = 'particle';
                particle.style.width = Math.random() * 60 + 20 + 'px';
                particle.style.height = particle.style.width;
                particle.style.left = Math.random() * 100 + '%';
                particle.style.top = Math.random() * 100 + '%';
                particle.style.background = colors[Math.floor(Math.random() * colors.length)];
                particle.style.animationDelay = Math.random() * 5 + 's';
                particle.style.animationDuration = Math.random() * 10 + 15 + 's';
                document.body.appendChild(particle);
            }
        }

        // Create sparkles on click
        document.addEventListener('click', (e) => {
            for (let i = 0; i < 5; i++) {
                const sparkle = document.createElement('div');
                sparkle.className = 'sparkle';
                sparkle.style.left = e.clientX + (Math.random() - 0.5) * 50 + 'px';
                sparkle.style.top = e.clientY + (Math.random() - 0.5) * 50 + 'px';
                sparkle.style.animationDelay = i * 0.1 + 's';
                document.body.appendChild(sparkle);
                
                setTimeout(() => sparkle.remove(), 2000);
            }
        });

        // Go back function
        function goBack() {
            if (window.history.length > 1) {
                window.history.back();
            } else {
                window.location.href = '/feed';
            }
        }

        // Initialize
        createParticles();

        // Emoji interaction - make them react to proximity
        const emojis = document.querySelectorAll('.floating-emoji');
        document.addEventListener('mousemove', (e) => {
            emojis.forEach(emoji => {
                const rect = emoji.getBoundingClientRect();
                const emojiX = rect.left + rect.width / 2;
                const emojiY = rect.top + rect.height / 2;
                const distance = Math.sqrt(
                    Math.pow(e.clientX - emojiX, 2) + 
                    Math.pow(e.clientY - emojiY, 2)
                );
                
                if (distance < 150) {
                    const angle = Math.atan2(e.clientY - emojiY, e.clientX - emojiX);
                    const push = (150 - distance) / 10;
                    emoji.style.transform = `translate(${-Math.cos(angle) * push}px, ${-Math.sin(angle) * push}px) scale(1.1)`;
                } else {
                    emoji.style.transform = '';
                }
            });
        });
    </script>
</body>
</html>
