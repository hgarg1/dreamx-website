# Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Microsoft (Entra ID - app registration supports personal+work)
MICROSOFT_CLIENT_ID=your-ms-client-id
MICROSOFT_CLIENT_SECRET=your-ms-client-secret
MICROSOFT_CALLBACK_URL=http://localhost:3000/auth/microsoft/callback

# Apple (requires Apple Developer program + HTTPS callback)
APPLE_CLIENT_ID=your-apple-service-id
APPLE_TEAM_ID=your-apple-team-id
APPLE_KEY_ID=your-apple-key-id
APPLE_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...key...\n-----END PRIVATE KEY-----\n
APPLE_CALLBACK_URL=https://your-domain.com/auth/apple/callback